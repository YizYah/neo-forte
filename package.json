{
  "name": "neo-forte",
  "version": "1.0.0",
  "description": "a simple querying interface for a neo4j database",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "commit": "git-cz",
    "coverage": "nyc npm test",
    "int-test": "ava --config int-tests.config.cjs",
    "lint": "eslint \"src/**/*.ts\"",
    "lintfix": "eslint \"src/**/*.ts\" --fix",
    "prepack": "rm -rf lib && tsc -b",
    "test": "ava"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "ava": "^3.15.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "ava": {
    "files": [
      "test/**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "neo-forgery": "^1.11.0",
    "neo4j-driver": "^4.3.4",
    "neo4j-driver-core": "^4.3.4"
  }
}
